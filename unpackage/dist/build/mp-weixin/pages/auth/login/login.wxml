    </image>
  </view>
  <view class="top flex-d al-center data-v-13cc7f0f">
    <u-form vue-id="09d99dd1-1" model="{{form}}" data-ref="uForm" class="data-v-13cc7f0f vue-ref" bind:__l="__l"
      vue-slots="{{['default']}}">
      <u-form-item vue-id="{{('09d99dd1-2')+','+('09d99dd1-1')}}" label prop="phone" class="data-v-13cc7f0f"
