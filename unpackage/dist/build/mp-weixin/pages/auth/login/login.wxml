        <view class="uiput flex al-center pos-rel data-v-13cc7f0f">
          <image class="iptimg pos-abs data-v-13cc7f0f"
            src="https://oss.kuaitongkeji.com/static/img/app/login/phone.png" mode></image>
          <u-input bind:input="__e" class="ipt data-v-13cc7f0f" vue-id="{{('09d99dd1-3')+','+('09d99dd1-2')}}"
            type="number" clearable="{{flag}}" placeholder="输入手机号" value="{{form.phone}}"
            data-event-opts="{{[['^input',[['__set_model',['$0','phone','$event',[]],['form']]]]]}}" bind:__l="__l">
          </u-input>
        </view>
      </u-form-item>
      <u-form-item vue-id="{{('09d99dd1-4')+','+('09d99dd1-1')}}" label prop="Verification" class="data-v-13cc7f0f"
        bind:__l="__l" vue-slots="{{['default']}}">
        <view class="uiput flex al-center pos-rel data-v-13cc7f0f">
          <image class="iptimg2 pos-abs data-v-13cc7f0f"
            src="https://oss.kuaitongkeji.com/static/img/app/login/security.png" mode></image>
          <u-input bind:input="__e" class="ipt data-v-13cc7f0f" vue-id="{{('09d99dd1-5')+','+('09d99dd1-4')}}"
            type="number" clearable="{{flag}}" placeholder="输入验证码" value="{{form.Verification}}"
            data-event-opts="{{[['^input',[['__set_model',['$0','Verification','$event',[]],['form']]]]]}}"
            bind:__l="__l"></u-input>
          <view data-ref="code" data-event-opts="{{[['tap',[['addvercode',['$event']]]]]}}"
            class="{{['vcode pos-abs flex al-center ju-center data-v-13cc7f0f vue-ref',(code===false)?'dv':'']}}"
            bindtap="__e">
            <view class="tetxs data-v-13cc7f0f">{{''+text+''}}</view>
          </view>
        </view>
        <u-toast vue-id="{{('09d99dd1-6')+','+('09d99dd1-4')}}" data-ref="uToast" class="data-v-13cc7f0f vue-ref"
