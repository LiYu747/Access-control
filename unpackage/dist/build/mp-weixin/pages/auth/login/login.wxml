  <view class="flex-d al-center data-v-13cc7f0f">
    <view data-event-opts="{{[['tap',[['Login',['$event']]]]]}}"
      class="btn pos-rel flex ju-center al-center data-v-13cc7f0f" bindtap="__e">
      <image class="jbsimg data-v-13cc7f0f" src="https://oss.kuaitongkeji.com/static/img/app/login/jbs.png" mode>
