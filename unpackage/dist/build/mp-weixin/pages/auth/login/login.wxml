    <view class="flex al-center retrieve data-v-13cc7f0f">
      <view data-event-opts="{{[['tap',[['find',['$event']]]]]}}" bindtap="__e" class="data-v-13cc7f0f">找回密码</view>
      <view class="line data-v-13cc7f0f"></view>
      <view data-event-opts="{{[['tap',[['register',['$event']]]]]}}" bindtap="__e" class="data-v-13cc7f0f">注册账号</view>
    </view>
