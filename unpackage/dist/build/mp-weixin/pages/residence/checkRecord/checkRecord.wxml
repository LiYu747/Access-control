<view class="flex-d al-center data-v-35c65667">
  <block wx:if="{{lists.length>0}}">
    <view class="data-v-35c65667">
      <block wx:for="{{$root.l0}}" wx:for-item="items" wx:for-index="indexs" wx:key="id">
        <view data-event-opts="{{[['tap',[['godetails',['$0'],[[['lists','id',items.$orig.id]]]]]]]}}"
          class="{{['card data-v-35c65667',(indexs==lists.length-1)?'bottom':'']}}" bindtap="__e">
          <view class="bx1 flex al-center pos-rel data-v-35c65667">
            <image class="dv3 data-v-35c65667" src="https://oss.kuaitongkeji.com/static/img/app/visit/user.png" mode>
            </image>
            <view class="data-v-35c65667">{{''+username+''}}</view>
            <view class=" pos-abs rig data-v-35c65667">{{''+items.$orig.verify_status_text+'\n\t\t\t\t\t>'}}</view>
          </view>
          <view class="bx1 flex al-center data-v-35c65667">
            <image class="dv3 data-v-35c65667" src="https://oss.kuaitongkeji.com/static/img/app/visit/time.png" mode>
            </image>
            <view class="data-v-35c65667">{{''+items.g0+''}}</view>
          </view>
          <view class="bx2 flex al-center data-v-35c65667">
            <image class="dv3 data-v-35c65667" src="https://oss.kuaitongkeji.com/static/img/app/visit/pos.png" mode>
            </image>
            <view class="data-v-35c65667">{{''+items.$orig.own_village.name+''}}</view>
          </view>
        </view>
      </block>
      <view hidden="{{!(isLoding==true&&lists.length>0)}}" class=" flex ju-center al-center lodbox data-v-35c65667">
        <image class="lodimg data-v-35c65667" src="https://oss.kuaitongkeji.com/static/img/app/address/loading.gif"
          mode></image>加载中...
      </view>
      <block wx:if="{{hasMore==false}}">
        <view class="flex ju-center m-b2 fz-14 data-v-35c65667">{{''+text+''}}</view>
      </block>
    </view>
  </block>
  <block wx:if="{{lists.length==0&&isLoding==false}}">
    <view class="nono flex al-center ju-center data-v-35c65667">您还没有申请记录哦~</view>
  </block>
  <view hidden="{{!(isLoding==true)}}" class="showloding flex al-center ju-center data-v-35c65667">
    <view class="loding flex-d al-center ju-center data-v-35c65667">
      <view class="  data-v-35c65667">
        <image class="loimg data-v-35c65667" src="https://oss.kuaitongkeji.com/static/img/app/address/loading.gif" mode>
        </image>
      </view>加载中
    </view>
  </view>
</view>